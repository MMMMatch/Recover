#include<stdio.h>
#include<iostream>
#include<vector>
using namespace std;
int main()
{
	vector<int>b;
	int num;
	while (cin >> num)
	{
		b.push_back(num);
		if (cin.get() == '\n')   //遇到回车，终止
			break;
	}
	int i;
	for (i = 0; i < b.size(); i++)
	{
		cout << b[i] << " ";
	}
	cout << b.size()<<endl;

	int m;
	int k;
	int j;
	for (m = 0; m < b.size(); m++)
	{
		cout << b[m] << " ";
	}
	for (j=b.size()-1; j > 0; j--)/*控制比较的轮次，总共为n-1次*/
	{
		for (k = 0; k < j; k++)/*对比次数,每次比较的次数为n-1次*/
		{
			int temp = 0;
			if (b[k] > b[k + 1])/*两个数字进行比较，若前一个数字比后一个数字大，将两个数字调换位置*/
			{
				temp = b[k];
				b[k] = b[k + 1];
				b[k + 1] = temp;
			}
		}
		for (k = 0; k < b.size(); k++)
		{
			cout << b[k] << " ";
		}
		cout << endl;
	}
	return 0;

}
