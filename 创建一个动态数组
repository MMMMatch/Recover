#include<stdio.h>
#include<iostream>
#include<vector>
using namespace std;
int main()
{
    vector<int>b;
		int num;
		while (cin >> num)
		{
			b.push_back(num);
			if (cin.get() == '\n')   //遇到回车，终止
				break;
		}
		int i;
		for (i = 0; i < b.size(); i++)
		{
			cout << b[i] << " ";
		}
		system("pause");
		return 0;
}
