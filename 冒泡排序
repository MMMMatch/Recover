#include<stdio.h>
#include<iostream>
using namespace std;
int main()
{
	int arr[8] = { 56,9,78,41,2,36,92,74 };
		int m;
		int i;
		int j;
		for (m = 0; m < 8; m++)
		{
			cout << arr[m] << " ";
		}
		cout << endl;
		for (j = 7; j > 0; j--)/*控制比较的轮次，总共为n-1次*/
		{
			for (i = 0; i < j; i++)/*对比次数,每次比较的次数为n-1次*/
			{
				int temp = 0;
				if (arr[i] > arr[i + 1])/*两个数字进行比较，若前一个数字比后一个数字大，将两个数字调换位置*/
				{
					temp = arr[i];
					arr[i] = arr[i + 1];
					arr[i + 1] = temp;
				}
			}
			for (i = 0; i < 8; i++)
			{
				cout << arr[i] << " ";
			}
			cout << endl;
		}
		return 0;
}
